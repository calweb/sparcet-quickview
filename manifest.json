{
  "name": "Sparcet Quickview",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Displays list of recent Sparcets.",
  "browser_action": {
    "default_icon":  "icon-48.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png" },
  "permissions": [
    "https://*.sparcet.com/"
  ]

}